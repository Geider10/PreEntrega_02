<div>
    <form id="formu">
        <div class="content_btn">
            <button id="btnAdd">Add Product</button>
            <button id="btnDelete">Delete Product</button>
        </div>
          <div>
            <label >Id: </label>
            <input type="text" id="iptId" name="id">
        </div>
        <div>
            <label >Nombre: </label>
            <input type="text" id="iptName" name="name">
        </div>
        <div class="content_btn">
            <button id="btnSend" type="submit">Send</button>
            <button id="btnCancel">Cancel</button>
        </div>
    </form>

    {{#each products}}
        <p>{{this.name}}</p>
    {{/each}}
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>